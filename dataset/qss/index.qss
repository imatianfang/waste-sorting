QPushButton{
    text-align : center;background-color : white;font: bold;border-color: gray;
    border-width: 2px;border-radius: 10px;padding: 6px;height : 50px;border-style: outset;font : 30px;
}

QPushButton:pressed{
    text-align : center;background-color : light gray;font: bold;border-color: gray;
    border-width: 2px;border-radius: 10px;padding: 6px;height : 50px;border-style: outset;font : 30px;
}

